
{
    "manifest_version": 2,
  
    "name": "The similarity of:",
    "description": "Execute script after click",
    "version": "1.0",
  
    "icons": {
      "128": "icon.png"
    },
  
    "permissions": [
      "tabs", "<all_urls>", "activeTab", "http://127.0.0.1:5000/"
    ],
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
  