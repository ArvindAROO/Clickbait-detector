
{
    "manifest_version": 2,
  
    "name": "Clickbait Detector",
    "description": "Has access to this site",
    "version": "1.0",
  
    "icons": {
      "48": "icon.png"
    },
  
    "permissions": [
      "tabs", "<all_urls>", "activeTab", "http://127.0.0.1:5000/"
    ],
  
    "browser_action": {
      "default_icon": {
        "48": "icon.png"
      },
      "default_popup": "popup.html"
    }
  }
  